            onAction="#testPick">

    </Button>
    <Button fx:id="Button3" text="#Callback_getAnswers"
            prefWidth="100" prefHeight="40" defaultButton="true"
            GridPane.columnIndex="35" GridPane.rowIndex="60"
            GridPane.columnSpan="200" GridPane.rowSpan="1"

            onAction="#testPick">

    </Button>
    <Button fx:id="Button4" text="#Callback_getAnswers"
